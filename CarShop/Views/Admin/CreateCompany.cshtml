@model CarShop.ViewModels.CreateCompanyViewModel;

@{
    ViewBag.Title = "Add Company";
}
<form asp-action="CreateCompany" method="post" asp-controller="Admin" class="login-registerform">
    <div class="content">
        <div asp-validation-summary="All"></div>
        <div>
            <label asp-for="CompanyName">CompanyName</label>
            <input type="text" asp-for="CompanyName" />
        </div>

        <div>
            <input type="submit" value="Add" class="loginregister-btn" />
        </div>
    </div>
</form>
